{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "name": "${mod_name}",
  "version": "${mod_version}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "sources": "${mod_repo_url}",
    "issues": "${mod_repo_url}/issues"
  },
  "description": "${mod_description}",
  "icon": "${mod_logo}",
  "license": "${mod_license}",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.iafenvoy.thinkbeforedrop._loader.fabric.ThinkBeforeDropFabricClient"
    ],
    "modmenu": [
      "com.iafenvoy.thinkbeforedrop._loader.fabric.compat.ModMenu"
    ]
  },
  "mixins": [
    "thinkbeforedrop.mixins.json"
  ],
  "depends": {
    "minecraft": [${supported_minecraft_versions}],
    "jupiter": ">=2.3"
  }
}